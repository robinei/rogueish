{
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "files.trimTrailingWhitespace": true,
	"files.exclude": {
		"**/.git": true,
		"**/.DS_Store": true,
        "**/*.js": { "when": "$(basename).ts" },
        "**/*.js.map": { "when": "$(basename).ts" },
        "build/": true,
        "bundle.js": true,
        "bundle.js.map": true,
        "compiler.jar": true
	}
}
